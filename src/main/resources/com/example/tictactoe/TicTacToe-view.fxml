<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tictactoe.TicTacToeController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <AnchorPane prefHeight="116.0" prefWidth="240.0">
      <children>
         <Button alignment="CENTER" layoutX="143.0" layoutY="81.0" mnemonicParsing="false" text="Next" />
       <Button layoutX="195.0" layoutY="81.0" mnemonicParsing="false" text="Reset" />
      
        <Label fx:id="welcomeText" layoutX="57.0" layoutY="14.0" text="Tic-Tac-Toe" textFill="#c95252">
            <font>
               <Font name="System Bold Italic" size="23.0" />
            </font>
         </Label>
         <Label layoutX="14.0" layoutY="43.0" text="X = ">
            <font>
               <Font name="System Bold" size="21.0" />
            </font>
         </Label>
         <Label layoutX="15.0" layoutY="74.0" text="Y = ">
            <font>
               <Font name="System Bold" size="21.0" />
            </font>
         </Label>
         <Label fx:id="xCounter" layoutX="57.0" layoutY="43.0" text="0">
            <font>
               <Font name="System Bold" size="21.0" />
            </font>
         </Label>
         <Label fx:id="yCounter" layoutX="57.0" layoutY="74.0" text="0">
            <font>
               <Font name="System Bold" size="21.0" />
            </font>
         </Label>
      </children>
   </AnchorPane>
   <AnchorPane maxWidth="-Infinity" prefHeight="240.0" prefWidth="200.0">
      <children>
         <GridPane gridLinesVisible="true">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0" minWidth="80.0" prefWidth="80.0" />
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0" minWidth="80.0" prefWidth="80.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="80.0" prefWidth="80.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="80.0" prefHeight="80.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="80.0" prefHeight="80.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="80.0" prefHeight="80.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
              <Button fx:id="button1" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="1" />
               <Button fx:id="button2" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="2" GridPane.columnIndex="1" />
               <Button fx:id="button3" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="3" GridPane.columnIndex="2" />
               <Button fx:id="button4" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="4" GridPane.rowIndex="1" />
               <Button fx:id="button5" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="5" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <Button fx:id="button6" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               <Button fx:id="button7" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="7" GridPane.rowIndex="2" />
               <Button fx:id="button8" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="8" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               <Button fx:id="button9" mnemonicParsing="false" onAction="#onHelloButtonAction" prefHeight="80.0" prefWidth="80.0" text="9" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            </children>
         </GridPane>
      </children>
   </AnchorPane>

</VBox>
